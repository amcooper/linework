<html>
	<head>
		<title>The LineHopper</title>
		<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=geometry,places"></script>
		<script type="text/javascript" src="/js/fp_main.js"></script>
		<meta charset="utf-8">
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap-theme.min.css">
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
		<link href="/css/fp_route.css" rel="stylesheet" type="text/css"> 
		<link href="//fonts.googleapis.com/css?family=Oleo Script&subset=latin" rel="stylesheet" type="text/css">
	</head>

	<body>
		<div class="container">
			<div id="titleBar" class="section">
				<form id="titleForm" action="/logout" method="GET">
					The LineHopper&nbsp;<button class="btn btn-sm btn-primary btn-block" type="submit">Log out</button>
				</form>
			</div>
			<div id="restfulRoutes" class="section">
				<p>
					<b>Top Twenty</b>
					<input type="text" id="search_term" placeholder="e.g. bakery">
					<button onclick="compileAll();">Search</button>&nbsp;<button id = "addNewRoute">ADD IT!</button>
				</p>

				<br /><br /> 
			</div>

	        <div id="directionsPanel">
	        	<ol id="searchResultList"></ol>
	        </div>
			<div id="googleMap"></div>
		</div>

		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
		<script type="text/javascript" src="/js/start.js"></script>
	</body>
</html>
